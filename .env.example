# Environment variables

# Multiple tunnels configuration (JSON format)
# Format: [{"name":"Tunnel Name","url":"https://..."},{"name":"Another Tunnel","url":"https://..."}]
TUNNELS=[{"name":"My Tunnel","url":"https://your-tunnel-url.trycloudflare.com/"}]

# Legacy single tunnel support (will be used if TUNNELS is not set)
TUNNEL_URL=https://your-tunnel-url.trycloudflare.com

# Telegram configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Monitor settings
CHECK_INTERVAL_MINUTES=5
TIMEOUT_MS=10000
